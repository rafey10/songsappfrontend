{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "target": "es2015"
    },
    "include": ["scripts/**/*"],
    "exclude": ["**/node_modules/*"]
  }
  